{
  "name": "weather_app",
  "version": "1.0.0",
  "description": "A Node.js/Express weather app with react",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client/ && npm install && npm run build"
  },
  "keywords": [
    "node.js",
    "express",
    "react.js"
  ],
  "author": "Madalin Preda",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "express-validator": "^6.4.0",
    "helmet": "^3.22.0",
    "node-cron": "^2.0.3",
    "path": "^0.12.7",
    "stream-json": "^1.5.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  },
  "engines": {
    "node": "10.20.0"
  }
}